<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
    <msapplication>
        <tile>
            <square70x70logo src="{{ (resources.Get "/icons/favicon-32x32.png").RelPermalink }}"/>
            <square150x150logo src="{{ (resources.Get "/icons/android-chrome-192x192.png").RelPermalink }}"/>
            <square310x310logo src="{{ (resources.Get "/icons/android-chrome-192x192.png").RelPermalink }}"/>
            <wide310x150logo src="{{ (resources.Get "/icons/android-chrome-512x512.png").RelPermalink }}"/>
            <TileColor>{{ .TileColor | default "#2d89ef" }}</TileColor>
        </tile>
    </msapplication>
</browserconfig>
